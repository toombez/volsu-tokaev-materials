{
  "name": "tokaev-volsu-slides",
  "version": "0.0.0",
  "type": "module",
  "keywords": [
    "slidev"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "slidev build",
    "dev": "slidev presentations/system-programming/full.md",
    "export": "slidev export presentations/system-programming/full.md"
  },
  "dependencies": {
    "@slidev/theme-default": "^0.25.0",
    "@slidev/types": "^0.49.29",
    "prism-theme-vars": "^0.2.5",
    "vue": "^3.5.3"
  },
  "devDependencies": {
    "@slidev/cli": "^0.49.29",
    "playwright-chromium": "^1.47.0"
  },
  "slidev": {
    "defaults": {
      "colorSchema": "both",
      "highlighter": "all",
      "author": "Тимур Токаев",
      "aspectRatio": "4/3",
      "transition": "slide-left",
      "canvasWidth": 680,
      "fonts": {
        "sans": "Roboto",
        "serif": "Roboto Slab",
        "mono": "Fira Code"
      },
      "htmlAttrs": {
        "dir": "ltr",
        "lang": "ru"
      },
      "download": true,
      "drawings": {
        "persist": false
      },
      "mdc": true
    }
  }
}